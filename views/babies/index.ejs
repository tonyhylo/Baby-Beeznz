<%- include('../partials/header') %>

<h1 class="h-title">Baby List</h1>
<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Date of Birth</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <!-- Write the line of EJS to iterate over movies using forEach -->
    <% baby.forEach(function(b) { %>
    <tr>
      <td><%= b.name %></td>
      <td><%= b.dob.toLocaleDateString() %></td>
      <td><a href="/babies/<%= b._id %>">DETAILS</a></td>
    </tr>
    <% }); %>
  </tbody>
</table>

<%- include('../partials/footer') %>
